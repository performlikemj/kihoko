<configuration>
  <appSettings>
    <add key="SCM_SCRIPT_GENERATOR_ARGS" value="--python"/>
    <add key="WSGI_HANDLER" value="kihokosite.wsgi.application"/>
    <add key="PYTHONPATH" value="D:\home\site\wwwroot"/>
  </appSettings>
  <system.webServer>
    <handlers>
      <add name="PythonHandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified"/>
    </handlers>
    <httpPlatform processPath="%PYTHON%\python.exe"
        arguments="D:\home\site\wwwroot\manage.py runserver %HTTP_PLATFORM_PORT%"
        stdoutLogEnabled="true"
        stdoutLogFile="\\?\%home%\LogFiles\python.log"
        startupRetryCount='10'>
    </httpPlatform>
  </system.webServer>
</configuration>
